{
  "name": "vertex-ai-utilities-api",
  "version": "1.1.0",
  "description": "Utilities API for Google Vertex AI integration with Express.js and Genkit",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "deploy": "pnpm build && wrangler deploy",
    "deploy:staging": "pnpm build && wrangler deploy --env staging"
  },
  "keywords": [
    "vertex-ai",
    "genkit",
    "express",
    "cloudflare",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@genkit-ai/ai": "^1.16.1",
    "@genkit-ai/core": "^1.16.1",
    "@genkit-ai/googleai": "^1.16.1",
    "@genkit-ai/vertexai": "^1.16.1",
    "@google-cloud/aiplatform": "^5.5.0",
    "@google-cloud/speech": "^6.4.0",
    "@google-cloud/text-to-speech": "^5.3.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "genkit": "^1.16.1",
    "helmet": "^8.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
